{
  "name": "etmodi",
  "version": "1.0.0",
  "description": "Rimble Icons",
  "main": "index.js",
  "module": "index.es.js",
  "sideEffects": false,
  "author": "LauryShields",
  "license": "MIT",
  "dependencies": {
    "@svgr/cli": "^4.3.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "grid-styled": "^4.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "material-design-icons": "^3.0.1",
    "ok-css": "^1.0.0-0",
    "pixo": "^1.1.0",
    "react": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "recursive-readdir": "^2.2.2",
    "styled-components": "^3.2.6",
    "styled-system": "^2.0.0"
  },
  "peerDependencies": {
    "styled-components": ">=2.0 || >=3.0"
  },
  "pixo": {
    "outDir": "source",
    "template": "styledSystem",
    "index": false,
    "iconComponent": true
  },
  "x0": {
    "cssLibrary": "styled-components"
  },
  "svgr": {
    "dimensions": false,
    "svgProps": {
      "viewBox": "0 0 24 24",
      "height": "{props.size}",
      "width": "{props.size}",
      "fill": "{ props.color ? props.color : '#000' }"
    },
    "svgoConfig": {
      "plugins": [
        {
          "convertColors": false
        }
      ]
    }
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "create-index": "^2.5.0",
    "cross-env": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NashKilback/etmodi.git"
  }
}